{
  "name": "connect-to-earn",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "start:local": "ts-node src/server/index.ts",
    "build:programs": "cd programs/swarm-network && cargo build-bpf",
    "deploy:testnet": "npm run build:programs && node scripts/deploy-testnet.js",
    "init:testnet": "ts-node --project tsconfig.scripts.json scripts/initialize-program.ts",
    "test:program": "ts-node --project tsconfig.scripts.json scripts/test-program.ts",
    "verify:testnet": "node scripts/verify-testnet.js",
    "test:testnet": "jest --config jest.testnet.config.js",
    "monitor:testnet": "node scripts/monitor-testnet.js"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.3.11",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-wallets": "^0.19.22",
    "@solana/web3.js": "^1.98.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/recharts": "^1.8.29",
    "@webgpu/types": "^0.1.34",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "clsx": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.284.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.8.1",
    "recharts": "^2.12.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.156.1",
    "util": "^0.12.5",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/bn.js": "^5.1.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^4.4.9",
    "vite-plugin-node-polyfills": "^0.23.0"
  }
